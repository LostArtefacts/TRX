        {
            "title": "Return to Egypt",
            "file": "data/egypt.phd",
            "type": "normal",
            "music": 59,
            "injections": [
                "data/egypt_fd.bin",
                "data/egypt_textures.bin",
            ],
            "sequence": [
                {"type": "start_game"},
                {"type": "loop_game"},
                {"type": "stop_game"},
                {"type": "level_stats", "level_id": 0},
                {"type": "exit_to_level", "level_id": 1},
            ],
            "strings": {"key1": "Gold Key"},
            "unobtainable_kills": 1,
        },

